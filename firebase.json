{
    "hosting": {
        "public": "build",
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }],
        "headers": [{
            "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
            "headers": [{
                "key": "Access-Control-Allow-Origin",
                "value": "*"
            }]
        }, {
            "source": "**/*.@(jpg|jpeg|gif|png|svg|eot|otf|ttf|ttc|woff|woff2|css|js)",
            "headers": [{
                "key": "Cache-Control",
                "value": "max-age=2592000"
            }]
        }]
    }
}
